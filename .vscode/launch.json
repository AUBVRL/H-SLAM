    "version": "0.2.0",
    "configurations": [
        {
            "name": "g++ build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/FSLAM",
            "args": [
                "sensor=Stereo", //Monocular, Stereo or RGBD
                "dataset=Euroc", //Euroc, TumMono or Kitti
                "photomCalibmodel=OnlineCalib", //OnlineCalib, HaveCalib, NoCalib
                "intrinsics=/media/gy/Drive/devel/FSLAM/Extras/Calib/EuRoC.yaml",
                "vocabulary=pathtovocab",
                "path=/media/gy/Drive/VSLAM_Datasets/EuroC/MH_01_easy",//Euroc //"path=/media/gy/Drive/VSLAM_Datasets/TUM_Mono/sequence_50/images.zip", //TumMono //"path=/media/gy/Drive/VSLAM_Datasets/Kitti/Sequence_00/", //Kitti      
                "timestampsL=pathtotimestamps",
                "gammaL=",
                "gammaR=",
                "vignetteL=",
                "vignetteR=",
                "start=0",
                "end=9999999",
                "reverse=0",
                "nogui=0",
                "prefetch=0",
                "playbackspeed=0"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        }
    ]
}